                  "pm.test(\"Status code should match the expected result\", function () { \r",
                  "    const expected = pm.environment.get(\"expectedStatusCode\"); \r",
                  "    pm.response.to.have.status(expected);\r",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
