				{
					"name": "Get Bookings",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if the response status code is 200\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.expect(pm.response.code).to.equal(200);\r",
									"});\r",
									"\r",
									"// Parse the response body as JSON\r",
									"const response = pm.response.json();\r",
									"\r",
									"// Validate the response structure\r",
									"pm.test(\"Response is an array\", function () {\r",
									"    pm.expect(response).to.be.an('array').that.is.not.empty;\r",
									"});\r",
									"\r",
									"// Loop through each booking to validate properties\r",
									"response.forEach(booking => {\r",
									"    pm.test(`Booking with ID ${booking.id} has required properties`, function () {\r",
									"        pm.expect(booking).to.have.all.keys('id', 'startDate', 'endDate', 'isActive', 'customerId', 'roomId', 'customer', 'room');\r",
									"        pm.expect(booking.startDate).to.be.a('string');\r",
									"        pm.expect(booking.endDate).to.be.a('string');\r",
									"        pm.expect(booking.isActive).to.be.a('boolean');\r",
									"        pm.expect(booking.customerId).to.be.a('number');\r",
									"        pm.expect(booking.roomId).to.be.a('number');\r",
									"\r",
									"        // Validate customer object\r",
									"        pm.expect(booking.customer).to.have.all.keys('id', 'name', 'email');\r",
									"        pm.expect(booking.customer.name).to.be.a('string');\r",
									"        pm.expect(booking.customer.email).to.be.a('string');\r",
									"\r",
									"        // Validate room object\r",
									"        pm.expect(booking.room).to.have.all.keys('id', 'description');\r",
									"        pm.expect(booking.room.description).to.be.a('string');\r",
									"    });\r",
									"});\r",
									"\r",
									"// Check the content-type header\r",
									"pm.test(\"Content-Type is application/json\", function () {\r",
									"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\r",
									"});\r",
									""
								],
