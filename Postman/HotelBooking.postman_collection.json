        {
          "name": "/Bookings",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "id": "c4ee3b13-c74f-40db-8d90-49db702e9a02",
                "exec": [
                  "const status = pm.iterationData.get('status') ?? 201;\r",
                  "const startOffset = pm.iterationData.get('startOffset') ?? 1;\r",
                  "const endOffset = pm.iterationData.get('endOffset') ?? 2;\r",
                  "\r",
                  "const randomId = Math.floor(Math.random() * 1000);\r",
                  "\r",
                  "function addDays(days) {\r",
                  "  var result = new Date();\r",
                  "  result.setDate(result.getDate() + days);\r",
                  "  return result.toISOString();\r",
                  "}\r",
                  "\r",
                  "let payload = '';\r",
                  "if (status !== 400) {\r",
                  "  payload = {\r",
                  "    id: randomId,\r",
                  "    startDate: addDays(Number(startOffset)),\r",
                  "    endDate: addDays(Number(endOffset)),\r",
                  "    isActive: true,\r",
                  "    customerId: randomId,\r",
                  "    roomId: 1,\r",
                  "    customer: {\r",
                  "      id: randomId,\r",
                  "      name: `Customer ${randomId}`,\r",
                  "      email: `customer${randomId}@example.com`\r",
                  "    },\r",
                  "    room: {\r",
                  "      id: 1,\r",
                  "      description: `Room 1 description`\r",
                  "    }\r",
                  "  };\r",
                  "  pm.request.body.raw = JSON.stringify(payload);\r",
                  "} else {\r",
                  "  pm.request.body.raw = '';\r",
                  "}\r",
                  "\r",
                  "pm.environment.set(\"expectedStatusCode\", status);"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
